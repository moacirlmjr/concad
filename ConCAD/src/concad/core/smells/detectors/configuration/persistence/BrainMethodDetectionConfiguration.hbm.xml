<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="concad.core.smells.detectors.configuration.BrainMethodDetectionConfiguration"
		table="BRAINMETHODCONF">
		<id name="id" type="java.lang.Long">
			<column name="BRAINMETHODCONF_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="LOC_Greater_VeryHigh"
			type="java.lang.Double">
			<column name="LOC_Greater_VeryHigh" />
		</property>
		<property generated="never" lazy="false" name="WMC_GreaterEqual_Many"
			type="java.lang.Double">
			<column name="WMC_GreaterEqual_Many" />
		</property>
		<property generated="never" lazy="false"
			name="MAXNESTING_GreaterEqual_DEEP" type="java.lang.Double">
			<column name="MAXNESTING_GreaterEqual_DEEP" />
		</property>
		<property generated="never" lazy="false" name="NOF_GreaterEqual_SMemCap"
			type="java.lang.Double">
			<column name="NOF_GreaterEqual_SMemCap" />
		</property>
		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>