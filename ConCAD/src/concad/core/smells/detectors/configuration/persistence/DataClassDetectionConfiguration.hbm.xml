<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="concad.core.smells.detectors.configuration.DataClassDetectionConfiguration"
		table="DATACLASSCONF">
		<id name="id" type="java.lang.Long">
			<column name="DATACLASSCONF_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="WOC_Less_OneThird"
			type="java.lang.Double">
			<column name="WOC_Less_OneThird" />
		</property>
		<property generated="never" lazy="false" name="NOAP_SOAP_Greater_Few"
			type="java.lang.Double">
			<column name="NOAP_SOAP_Greater_Few" />
		</property>
		<property generated="never" lazy="false" name="WMC_Less_High"
			type="java.lang.Double">
			<column name="WMC_Less_High" />
		</property>
		<property generated="never" lazy="false" name="NOAP_SOAP_Greater_Many"
			type="java.lang.Double">
			<column name="NOAP_SOAP_Greater_Many" />
		</property>
		<property generated="never" lazy="false" name="WMC_Less_VeryHigh"
			type="java.lang.Double">
			<column name="WMC_Less_VeryHigh" />
		</property>
		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>