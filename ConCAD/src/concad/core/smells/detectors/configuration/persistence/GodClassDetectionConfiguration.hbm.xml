<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="concad.core.smells.detectors.configuration.GodClassDetectionConfiguration"
		table="GODCLASSCONF">
		<id name="id" type="java.lang.Long">
			<column name="GODCLASSCONF_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="ATFD_Greater_FEW"
			type="java.lang.Double">
			<column name="ATFD_Greater_FEW" />
		</property>
		<property generated="never" lazy="false" name="TCC_Less_OneThird"
			type="java.lang.Double">
			<column name="TCC_Less_OneThird" />
		</property>
		<property generated="never" lazy="false" name="WMC_GreaterEqual_VeryHigh"
			type="java.lang.Double">
			<column name="WMC_GreaterEqual_VeryHigh" />
		</property>
		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>