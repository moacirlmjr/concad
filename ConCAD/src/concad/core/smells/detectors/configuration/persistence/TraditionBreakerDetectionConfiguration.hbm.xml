<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="concad.core.smells.detectors.configuration.TraditionBreakerDetectionConfiguration"
		table="TRADITIONBREAKERCONF">
		<id name="id" type="java.lang.Long">
			<column name="TRADITIONBREAKERCONF_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="NAS_GreaterEqual_NOMAvg"
			type="java.lang.Double">
			<column name="NAS_GreaterEqual_NOMAvg" />
		</property>
		<property generated="never" lazy="false"
			name="PNAS_GreaterEqual_TWO_THIRD" type="java.lang.Double">
			<column name="PNAS_GreaterEqual_TWO_THIRD" />
		</property>
		<property generated="never" lazy="false" name="AMW_Greater_AMWAvg"
			type="java.lang.Double">
			<column name="AMW_Greater_AMWAvg" />
		</property>
		<property generated="never" lazy="false" name="WMC_GreaterEqual_VeryHigh"
			type="java.lang.Double">
			<column name="WMC_GreaterEqual_VeryHigh" />
		</property>

		<property generated="never" lazy="false" name="NOM_GreatherEqual_High"
			type="java.lang.Double">
			<column name="NOM_GreatherEqual_High" />
		</property>
		<property generated="never" lazy="false" name="AMW_Greater_AMWAvg_2"
			type="java.lang.Double">
			<column name="AMW_Greater_AMWAvg_2" />
		</property>
		<property generated="never" lazy="false" name="WMC_Greater_VeryHighDiv2"
			type="java.lang.Double">
			<column name="WMC_Greater_VeryHighDiv2" />
		</property>
		<property generated="never" lazy="false" name="NOM_GreatherEqual_HighDiv2"
			type="java.lang.Double">
			<column name="NOM_GreatherEqual_HighDiv2" />
		</property>
		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>