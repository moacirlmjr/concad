<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="concad.core.smells.detectors.configuration.FeatureEnvyDetectionConfiguration"
		table="FEATUREENVYCONF">
		<id name="id" type="java.lang.Long">
			<column name="FEATUREENVYCONF_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="ATFD_Greater_Few"
			type="java.lang.Double">
			<column name="ATFD_Greater_Few" />
		</property>
		<property generated="never" lazy="false" name="LAA_Less_OneThird"
			type="java.lang.Double">
			<column name="LAA_Less_OneThird" />
		</property>
		<property generated="never" lazy="false" name="FDP_LessEqual_FEW"
			type="java.lang.Double">
			<column name="FDP_LessEqual_FEW" />
		</property>
		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>