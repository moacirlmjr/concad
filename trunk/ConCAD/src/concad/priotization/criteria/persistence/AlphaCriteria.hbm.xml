<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="concad.priotization.criteria.AlphaCriteria" table="ALPHACRITERIA">
		<id name="id" type="java.lang.Long">
			<column name="ALPHACRITERIA_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="alpha"
			type="java.lang.Double">
			<column name="ALPHAVALUE" />
		</property>

		<property generated="never" lazy="false" name="projectName"
			type="java.lang.String">
			<column name="PROJECTNAME" />
		</property>
	</class>
</hibernate-mapping>