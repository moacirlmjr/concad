<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="concad.priotization.criteria.util.NOMOfAClass"
		table="NOMOFACLASS">
		<id name="id" type="java.lang.Long">
			<column name="NOMOFACLASS_ID" />
			<generator class="increment" />
		</id>
		<property generated="never" lazy="false" name="aClassName"
			type="java.lang.String">
			<column name="NAME" />
		</property>
		<property generated="never" lazy="false" name="nomValue"
			type="java.lang.Integer">
			<column name="NOMVALUE" />
		</property>
	</class>
</hibernate-mapping>